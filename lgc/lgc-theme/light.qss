QWidget{
  font-family: "Kranky";
  background-color: white;
  background-image: url("./lgc-icons/lantern-1000x1000.png");
  color: black;
}
QMenuBar{
  background-color: gray;
  color: white;
}
QMenuBar::item:selected{
  background-color: white;
  color: black;
}
QMenu{
  background-color: gray;
  color: white;
}
QMenu::item:selected{
  background-color: white;
  color: black;
}
QLabel{
  background-color: gray;
  border-radius: 10px;
  color: white;
  padding: 10px;
}
QPushButton{
  background-color: white;
  color: black;
  padding: 5px;
  border-radius: 5px;
  border-width: 1px;
  border-style: solid;
  border-color: red;
}
QPushButton:hover{
  background-color: #FFF2F2;
  text-decoration: "underline";
}
